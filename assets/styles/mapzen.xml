  <style-line id="trunk" stroke="#ebf0d8" width="1.5" />
  <style-line id="motorway" stroke="#f3f7e6" width="3" cap="butt" priority="1"/>
  <style-line id="primary" stroke="#f3f7e6" width="2" cap="butt" />
  <style-line id="secondary" use="primary" width="-0.2" />
  <style-line id="tertiary" use="primary" width="-0.9" />
  <style-line id="motorway:z7" stroke="#d6dac2" width="0.8" cap="butt" faded="7" />
  <style-line id="fix" width="1.0" cap="butt" fix="true" />
  <style-line id="park-line" stroke="#898b61" width="0.2" cap="butt" />

  <style-text id="caption-tiny-blue" k="name" size="12" fill="#7b91a4" stroke="#e8ecd3" stroke-width="1.0" />
  <style-text id="caption-small-blue" k="name" size="16" fill="#7b91a4" stroke="#e8ecd3" stroke-width="2.0" />
  <style-text id="park-label" k="name" size="18" fill="#38678F" stroke="#e8ecd3" stroke-width="1.0" priority="4" />
  <style-text id="street-label-sm" k="name" size="16" fill="#7b91a4" stroke="#e8ecd3" stroke-width="2.0" priority="3" />
  <style-text id="street-label" k="name" size="20" character-spacing="0.3" fill="#465b73" stroke="#e8ecd3" stroke-width="2.0" priority="2" />
  <style-text id="street-label-lg" k="name" size="28" fill="#7d8f9e" stroke="#e8ecd3" stroke-width="2.0" priority="1" />

  <style-text id="poi-label" k="name" size="16" fill="#373737" stroke="#e8ecd3" stroke-width="0.25" />


  <!-- ways -->
  <m e="way">

    <!-- land base -->
    <m k="land">
      <m v="base" zoom-min="0">
        <area src="patterns/dotsv4.png"/>
      </m>
    </m>


    <!-- parks, playgrounds + labels -->
    <m k="landuse">
      <m select="first">
        <m v="military" fade="9">
          <area src="patterns/park_texture.png" />
          <line use="park-line" />
        </m>
        <m v="residential|farmyard" fade="9">
          <area src="patterns/park_texture.png" />
          <line use="park-line" />
        </m>
        <m v="retail">
          <area fill="#ebf0d8" stroke="#ebf0d8" stroke-width="0.2" />
        </m>
        <m v="industrial|brownfield|railway">
          <area fill="#ebf0d8" stroke="#dcdbb3" stroke-width="0.2" />
        </m>
        <m v="commercial">
          <area fill="#ebf0d8" stroke="#ebf0d8" stroke-width="0.2" />
        </m>
        <m v="construction|greenfield">
          <area fill="#a47c41" stroke="#ebf0d8" stroke-width="0.2" />
        </m>
        <m v="garages">
          <area fill="#d6d6e4" />
        </m>
        <m v="landfill|quarry">
          <area fill="#ebf0d8" stroke="#d4d8c0" stroke-width="0.2" />
        </m>
        <m v="cemetery" fade="9">
          <area src="patterns/park_texture.png" />
          <line use="park-line" />
        </m>
        <m v="field|farm|farmland|meadow|grass" fade="9">
          <area src="patterns/park_texture.png" />
          <line use="park-line" />
        </m>
        <m v="allotments|village_green|recreation_ground" fade="9">
          <area src="patterns/park_texture.png" />
          <line use="park-line" />
        </m>
        <m v="reservoir|basin">
          <area fill="#8db3b6" stroke="#243F3F" stroke-width="0.2" />
        </m>
      </m>
      <m k="name" zoom-min="18">
        <text use="caption-small-blue" />
      </m>
    </m>


    <m k="natural|landuse" v="forest|wood|scrub">
      <m k="wood">
        <m k="wood" v="coniferous" fade="9">
          <area src="patterns/park_texture.png" />
          <line use="park-line" />
        </m>
        <m k="wood" v="deciduous" fade="9">
          <area src="patterns/park_texture.png" />
          <line use="park-line" />
        </m>
        <m k="wood" v="mixed" fade="9">
          <area src="patterns/park_texture.png" />
          <line use="park-line" />
        </m>
      </m>
      <m k="wood" v="~" fade="9">
        <area src="patterns/park_texture.png" />
      </m>
      <m zoom-min="15">
        <text use="caption-small-blue" />
      </m>
    </m>


    <!-- amenity -->
    <m k="amenity">
      <m select="first">
        <m v="kindergarten|school|college|university">
          <area fill="#ebf0d8" stroke="#d4d8c0" stroke-width="0.2" />
        </m>
        <m v="grave_yard" fade="9">
          <area src="patterns/park_texture.png" />
          <line use="park-line" />
        </m>
        <m v="parking">
          <area fill="#ebf0d8" stroke="#d4d8c0" stroke-width="0.2" />
          <m k="access" v="private" zoom-min="15">
             <area fill="#ebf0d8" stroke="#d4d8c0" stroke-width="0.2" />
          </m>
        </m>
        <m v="fountain" closed="yes">
          <area fill="#8db3b6" stroke="#243F3F" stroke-width="0.15" />
        </m>
        <m v="hospital" zoom-min="16">
          <symbol src="hospital" />
        </m>
        <m v="theatre" zoom-min="16">
          <symbol src="arts-ent" />
        </m>
        <m v="toilets" zoom-min="16">
          <symbol src="bathroom" />
        </m>
      </m>
      <m k="name" zoom-min="17">
        <text use="poi-label" />
      </m>
    </m>


    <!-- natural -->
    <m k="natural">
      <m select="first">
        <m v="coastline">
          <!-- <m closed="no">
            <line stroke="#708599" fix="true" cap="butt" width="2.0" />
            </m> -->
          <m closed="yes">
            <area fill="#8db3b6" stroke="#243F3F" stroke-width="0.4" />
          </m>
        </m>
      </m>
    </m>
    <m k="natural" zoom-min="10">
      <m v="glacier">
        <area fill="#8db3b6" stroke="#243F3F" stroke-width="0.8" />
      </m>
      <m v="land">
        <area fill="#f8f8f8" stroke="#e0e0e0" stroke-width="0.1" />
      </m>
      <m v="beach">
        <area fill="#eecc55" />
      </m>
      <m v="heath">
        <area fill="#ebf0d8" stroke="#ffff90" stroke-width="0.2" />
      </m>
      <m v="marsh|wetland">
        <area fill="#8db3b6" />
      </m>
      <m k="name" zoom-min="16">
        <text use="caption-small-blue" />
      </m>
    </m>


    <!-- leisure -->
    <m k="leisure">
        <!-- park labels -->
        <m v="park|garden|golf_course|common|green" fade="9">
          <area src="patterns/park_texture.png" />         
          <m zoom-min="11">
            <line stroke="#898b61" width="0.3" cap="butt" />
          </m>
          <m zoom-min="15">
            <text use="park-label" />
          </m>
        </m>
        <m v="playground|playing_fields|pitch|dog_park" fade="9">
          <m zoom-min="13">
            <area src="patterns/park_texture.png" />
          </m>
          <m zoom-min="15">
            <line stroke="#898b61" width="0.3" cap="butt" />
          </m>
        </m>
        <m v="stadium" fade="9">
          <area src="patterns/park_texture.png" />
          <line use="park-line" />
        </m>
        <m v="sports_centre|water_park" fade="9">
          <area src="patterns/park_texture.png" />
          <line use="park-line" /> />
        </m>
        <m v="track">
          <m k="area" v="yes|true">
            <area fill="#e8ecd3" stroke="#a3a27c" stroke-width="0.025" />
          </m>
          <m k="area" v="~|no|false">
            <line stroke="#000000" width="0.75" />
            <line stroke="#000000" width="0.7" />
          </m>
        </m>
        <m v="swimming_pool">
          <area src="patterns/water_texture.png" fill="#8DB4B7" />
        </m>
    </m>
    <m k="leisure|landuse" zoom-min="14">
      <m v="nature_reserve" fade="9">
        <area src="patterns/park_texture.png" />
        <line use="park-line" />
      </m>
    </m>

    <!-- waterways -->
    <m k="waterway">
      <!-- waterway casings -->
      <m v="water" zoom-min="0">
        <area src="patterns/water_texture.png" fill="#8DB4B7" />
        <line stroke="#243F3F" width="0.25" cap="butt" />
        <m zoom-min="13">
          <caption k="name" size="18" fill="#373737" />
        </m>
      </m>

      <!-- waterway cores -->
      <m v="ditch">
        <line stroke="#243F3F" width="0.2" />
      </m>
      <m v="drain">
        <line stroke="#243F3F" width="0.2" fix="true" fade="13" />
      </m>
      <!-- canal zoom levels -->
      <m v="canal" zoom-min="0">
        <area src="patterns/water_texture.png" fill="#8DB4B7"/>
        <line stroke="#243F3F" width="0.25" cap="butt" />
        <m zoom-min="16">
          <caption k="name" size="18" fill="#373737" />
        </m>
      </m>

      <!-- stream zoom levels -->
      <m v="stream">
        <area src="patterns/water_texture.png" fill="#8DB4B7"/>
        <line stroke="#243F3F" width="0.25" cap="butt" />
      </m>

      <!-- river zoom levels -->
      <m v="river" select="first">
        <area src="patterns/water_texture.png" fill="#8DB4B7"/>
        <line stroke="#243F3F" width="0.25" cap="butt" />
        <m v="river" zoom-min="14">
          <text use="street-label-sm" />
        </m>
      </m>

      <m v="dock">
        <area src="patterns/water_texture.png" fill="#8DB4B7"/>
        <line stroke="#243F3F" width="0.1" />
      </m>
      <m v="riverbank">
        <area src="patterns/water_texture.png" fill="#8DB4B7"/>
        <line stroke="#243F3F" width="0.1" />
      </m>
      <m v="weir">
        <line stroke="#243F3F" width="0.1" />
      </m>
      <m v="dam">
        <line stroke="#243F3F" width="0.1" />
      </m>
      <m k="lock" v="yes|true">
        <line stroke="#243F3F" width="0.1" cap="round" />
      </m>
    </m>


    <!-- military -->
    <m k="military">
      <area src="patterns/park_texture.png" />
      <line stroke="#898b61" width="0.2" cap="butt" />
      <caption k="name" size="18" fill="#373737" />
    </m>


    <!-- sport -->
    <m k="sport">
      <m v="soccer" zoom-min="16">
        <symbol src="soccer" />
      </m>
      <m v="swimming|canoe|diving|scuba_diving">
        <area fill="#8db3b6" stroke="#243F3F" stroke-width="0.2" />
      </m>
      <m v="tennis">
        <area src="patterns/park_texture.png" />
        <line stroke="#898b61" width="0.2" cap="butt" />
        <m zoom-min="17">
          <symbol src="sports" />
        </m>
      </m>
    </m>


    <!-- tourism areas -->
    <m k="tourism">
      <m v="attraction">
        <area fill="#f2caea" />
      </m>
      <m v="zoo|picnic_site|caravan_site|camp_site">
        <area src="patterns/park_texture.png" />
        <line stroke="#898b61" width="0.2" cap="butt" />
      </m>
      <m k="name" zoom-min="16">
        <text use="caption-small-blue" />
      </m>
    </m>

  <!-- water stroke at different zoom levels -->
    <m k="natural">
      <m v="water" zoom-min="0">
        <area src="patterns/water_texture.png"/>
        <line stroke="#243F3F" width="0.25" cap="butt" />
      </m>
    </m>

    <m k="natural" v="coastline" closed="no">
      <line stroke="#243F3F" fix="true" cap="butt" width="1.0" />
    </m>

    <!-- tunnel -->
    <m k="tunnel" v="true|yes">
      <!-- highway tunnels -->
      <m k="highway">
        <!-- highway tunnel casings -->
        <m v="steps|footway|path">
          <line stroke="#ebf0d8" width="0.6" dasharray="5,5" cap="round" />
        </m>
        <m v="track|cycleway|bridleway|service">
          <line stroke="#56452e" dasharray="5,5" cap="round" />
        </m>
        <m v="construction">
          <line stroke="#ebf0d8" width="1.1" dasharray="5,5" cap="round" />
        </m>
        <m v="road">
          <line stroke="#d6dac2" width="1.3" dasharray="5,5" cap="round" />
        </m>
        <m v="pedestrian|unclassified|residential|living_street|byway">
          <line stroke="#ebf0d8" width="1.5" dasharray="5,5" cap="round" />
        </m>
        <m v="tertiary|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|trunk">
          <line stipple-stroke="#A7A89B" width="2.6" stroke="#00A7A89B" stipple="2" stipple-width="1.5" cap="round" fix="true" />
        </m>

        <!-- highway tunnel cores -->
        <m v="steps|footway|path">
          <line stroke="#d6dac2" width="0.45" cap="round" />
        </m>
        <m v="track">
          <line stroke="#d6dac2" width="0.7" cap="round" />
        </m>
        <m v="cycleway|bridleway|service">
          <line stroke="#56452e" dasharray="5,5" cap="round" />
        </m>
        <m v="construction">
          <line stroke="#d6dac2" width="1.0" cap="round" />
        </m>
        <m v="road">
          <line stroke="#d6dac2" width="1.25" cap="round" />
        </m>
        <m v="pedestrian|unclassified|residential|living_street|byway">
          <line stroke="#d6dac2" width="1.35" cap="round" />
        </m>
        <m v="tertiary|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|trunk">
          <line stroke="#d6dac2" width="1.5" cap="round" />
          <m zoom-min="11">
            <text use="street-label" />
          </m>
        </m>
      </m>

      <!-- railway tunnel -->
      <m k="railway">
        <m v="tram|subway|light_rail|narrow_gauge">
          <line stroke="#dcdbb3" width="2" dasharray="4,2" cap="butt" />
        </m>
        <m v="rail">
          <line stroke="#dcdbb3" width="2" dasharray="4,2" cap="butt" />
        </m>
      </m>
    </m>


    <!-- highway area -->
    <m k="area" v="yes|true">
      <m k="highway">
        <m v="footway|footpath">
          <area fill="#f3f7e6" stroke="#707070" stroke-width="1.15" />
        </m>
        <m v="pedestrian">
          <area stroke="#f3f7e6" stroke-width="1.15" />
        </m>
        <m v="path">
          <area stroke="#f3f7e6" stroke-width="1.15" />
        </m>
        <m v="service">
          <area stroke="#707070" stroke-width="1.15" />
        </m>
        <m v="unclassified">
          <area stroke="#707070" stroke-width="1.3" />
        </m>
        <m v="residential">
          <area stroke="#707070" stroke-width="1.15" />
        </m>
        <m v="road">
          <area stroke="#f3f7e6" stroke-width="1.15" />
        </m>
        <m v="living_street">
          <area stroke="#f3f7e6" stroke-width="1.15" />
        </m>
      </m>
    </m>


    <!-- platform cores -->
    <m k="highway|railway|public_transport" v="platform">
      <m closed="yes">
        <area fill="#9b9b79" />
      </m>
      <m closed="no">
        <line stroke="#9b9b79" width="0.3" />
      </m>
    </m>


    <!-- runways casings and areas -->
    <m k="aeroway">
      <m v="aerodrome" closed="yes">
